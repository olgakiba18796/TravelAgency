{
  "name": "green",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/index.scss css/index.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/index.scss css/index.comp.css",
    "concat:css": "concat -o css/index.concat.css css/styles.css css/index.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/index.concat.css -o css/index.prefix.css",
    "compress:css": "node-sass css/index.prefix.css css/index.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
